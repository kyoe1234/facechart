<?xml version="1.0" encoding="UTF-8"?>
<axplugin id="ax.ext.net" version="1.1">
	<description>Networking Extension API Appspresso Plugin
	</description>
	<url>http://appspresso.com</url>
	<author>Appspresso Dev. Team</author>
	<license>Copyright (c) 2011, KT Hitel Co., LTD. All Rights Reserved.
	</license>

	<feature id="http://appspresso.com/api/ax.ext.net" category="Extension" />

	<module platform="android" platform-version="8"
		min-platform-version="7" max-platform-version=""
		class="com.appspresso.screw.net.NetPlugin">
		<property name="permission" value="android.permission.ACCESS_NETWORK_STATE" />
	</module>

	<module platform="ios" platform-version="4.1"
		min-platform-version="4.0" max-platform-version="" class="ax_ext_net_MyPlugin">
		<property name="framework" value="MessageUI.framework" />
	</module>
</axplugin>
